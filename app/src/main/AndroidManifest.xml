<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.promillrechner_mobapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".calculator.Diagram"></activity>
        <activity android:name=".calculator.Results"
            android:launchMode="singleTask"/>
        <activity android:name=".calculator.Alcohol"
            android:launchMode="singleTask"/>
        <activity android:name=".calculator.SelectPerson"
            android:launchMode="singleTask"/>
        <activity android:name=".database.PersonCreate" />
        <activity android:name=".database.PersonEdit" />
        <activity android:name=".database.PersonList" />
        <activity android:name=".Information" />
        <activity android:name=".Launcher">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>